@model HomeIndexViewModel

@{
    ViewBag.Title = "Material Control";
}

<h1>All Projects</h1>
<table>
    @foreach (var pj in Model.Projects) {
        <tr>
            <td>@pj.Id</td>
            <td>@pj.Name</td>
            <td>
                <a asp-action="Details" asp-route-id="@pj.Id">More</a>
            </td>
        </tr>
    }
</table>
<a asp-action="Create">Add Project</a>

@section footer{
    <h5>@Model.CurrentMessage</h5>
}